{
  "$schema": "https://qooxdoo.org/schema/Manifest-2-0-0.json",
  "info": {
    "name": "ville.Clean",
    "summary": "a theme for qooxdoo desktop applications inspired by Semantic UI",
    "description": "A theme inspired by Semantic UI designed to include more space (whitespace) and to be out of the way as much as possible",
    "homepage": "http://github.com/sqville/ville.Clean",
    "license": "MIT",
    "authors": [
      {
        "name": "Chris Eskew",
        "email": "sqville@gmail.com"
      }
    ],
    "version": "3.0.0"
  },
  "provides": {
    "namespace": "ville.theme.Clean",
    "encoding": "utf-8",
    "class": "source/class",
    "resource": "source/resource",
    "translation": "source/translation",
    "application": {
      "class": "widgetbrowser.Application",
      "theme": "ville.theme.Clean",
      "name": "widgetbrowser",
      "title": "WidgetBrowser",
      "include": [
        "ville.theme.CleanDark",
        "qx.theme.Simple",
        "ville.theme.clean.Image",
        "qx.theme.Classic",
        "widgetbrowser.pages.Tree",
        "widgetbrowser.pages.List",
        "widgetbrowser.pages.Table",
        "widgetbrowser.pages.Form",
        "widgetbrowser.pages.ToolBar",
        "widgetbrowser.pages.Window",
        "widgetbrowser.pages.Tab",
        "widgetbrowser.pages.Control",
        "widgetbrowser.pages.Embed",
        "widgetbrowser.pages.EmbedFrame",
        "widgetbrowser.pages.Basic",
        "widgetbrowser.pages.Misc"
      ],
      "exclude": [
        "qx.theme.Modern"
      ]
    },
    "fonts": {
      "LatoWeb": {
        "fontFaces": [
          {
            "fontFamily": "LatoWeb",
            "fontStyle": "normal",
            "fontWeight": "normal",
            "paths": [
              "ville/theme/clean/font/Lato/Lato-Regular.ttf",
              "ville/theme/clean/font/Lato/Lato-Regular.woff2",
              "ville/theme/clean/font/Lato/Lato-Regular.woff"
            ]
          }
        ]
      },
      "LatoWebSemibold": {
        "fontFaces": [
          {
            "fontFamily": "LatoWebSemibold",
            "fontStyle": "normal",
            "fontWeight": "normal",
            "paths": [
              "ville/theme/clean/font/Lato/Lato-Semibold.ttf",
              "ville/theme/clean/font/Lato/Lato-Semibold.woff2",
              "ville/theme/clean/font/Lato/Lato-Semibold.woff"
            ]
          }
        ]
      },
      "LatoWebBlack": {
        "fontFaces": [
          {
            "fontFamily": "LatoWebBlack",
            "fontStyle": "normal",
            "fontWeight": "normal",
            "paths": [
              "ville/theme/clean/font/Lato/Lato-Black.ttf",
              "ville/theme/clean/font/Lato/Lato-Black.woff2",
              "ville/theme/clean/font/Lato/Lato-Black.woff"
            ]
          }
        ]
      },
      "LatoWebBold": {
        "fontFaces": [
          {
            "fontFamily": "LatoWebBold",
            "fontStyle": "normal",
            "fontWeight": "normal",
            "paths": [
              "ville/theme/clean/font/Lato/Lato-Bold.ttf",
              "ville/theme/clean/font/Lato/Lato-Bold.woff2",
              "ville/theme/clean/font/Lato/Lato-Bold.woff"
            ]
          }
        ]
      }
    }
  },
  "requires": {
    "@qooxdoo/framework": "^7.8.0",
    "qooxdoo/qxl.formdemo": "^2.0.0",
    "qooxdoo/qxl.versionlabel": "^2.0.0"
  }
}