{
  "$schema": "https://qooxdoo.org/schema/Manifest-1-0-0.json",
  "info": {
    "name": "ville.Clean",
    "summary": "a theme for qooxdoo desktop applications inspired by Semantic UI",
    "description": "A theme inspired by Semantic UI designed to include more space (whitespace) and to be out of the way as much as possible",
    "homepage": "http://github.com/sqville/ville.Clean",
    "license": "MIT",
    "authors": [
      {
        "name": "Chris Eskew",
        "email": "chris.eskew@sqville.com"
      }
    ],
    "version": "1.0.3"
  },
  "provides": {
    "namespace": "ville.theme.Clean",
    "encoding": "utf-8",
    "class": "source/class",
    "resource": "source/resource",
    "translation": "source/translation",
    "application": {
      "class": "widgetbrowser.Application",
      "theme": "ville.theme.Clean",
      "name": "widgetbrowser",
      "title": "WidgetBrowser",
      "include": [
        "qx.theme.Simple",
        "qx.theme.Classic",
        "widgetbrowser.pages.Tree",
        "widgetbrowser.pages.List",
        "widgetbrowser.pages.Table",
        "widgetbrowser.pages.Form",
        "widgetbrowser.pages.ToolBar",
        "widgetbrowser.pages.Window",
        "widgetbrowser.pages.Tab",
        "widgetbrowser.pages.Control",
        "widgetbrowser.pages.Embed",
        "widgetbrowser.pages.EmbedFrame",
        "widgetbrowser.pages.Basic",
        "widgetbrowser.pages.Misc",
        "widgetbrowser.pages.Extras"
      ],
      "exclude": [
        "qx.theme.Modern"
      ]
    }
  },
  "requires": {
    "@qooxdoo/compiler": "^1.0.0",
    "@qooxdoo/framework": "^6.0.0",
    "qooxdoo/qxl.formdemo": "^1.0.2",
    "qooxdoo/qxl.versionlabel": "^1.0.3"
  }
}